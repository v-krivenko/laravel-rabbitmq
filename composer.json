{
    "name": "v-krivenko/laravel-rabbitmq",
    "description": "Simple rabbitmq integration for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Tilita",
            "email": "adrian@tilita.ro",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
	    "php-amqplib/php-amqplib": "3.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.*",
        "squizlabs/php_codesniffer": "2.*",
        "phpmd/phpmd" : "@stable",
        "laravel/framework" : "6.*|7.*|8.*|9.*"
    },
    "autoload": {
        "psr-4": {"NeedleProject\\LaravelRabbitMq\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": { "Tests\\NeedleProject\\LaravelRabbitMq\\": "tests/" }
    },
	"extra": {
		"laravel": {
			"providers": [
				"NeedleProject\\LaravelRabbitMq\\Providers\\ServiceProvider"
			]
		}
	},
    "minimum-stability": "dev",
    "prefer-stable": true
}
